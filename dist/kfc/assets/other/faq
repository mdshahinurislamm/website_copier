    function postHamburgerClick() {
        var dictionayData = {"key": "isHamMenuClicked", "value": "Y"};
        if ($('#hdnDeviceType').val() !== undefined && $('#hdnDeviceType').val() !== '') {
            if ($('#hdnDeviceType').val().toLowerCase() === "ios") {
                window.webkit.messageHandlers.invokeAction.postMessage(JSON.stringify(dictionayData));
            }
            else {
                jsBridge.invokeAction(JSON.stringify(dictionayData));
            }
        }
    }

    function PostLogin(name) {
        var dictionayData = {"key": "Name", "value": name};
        if ($('#hdnDeviceType').val() !== undefined && $('#hdnDeviceType').val() !== '') {
            if ($('#hdnDeviceType').val().toLowerCase() === "ios") {
                window.webkit.messageHandlers.invokeAction.postMessage(JSON.stringify(dictionayData));
            }
            else {
                jsBridge.invokeAction(JSON.stringify(dictionayData));
            }
        }
    }

</script>


