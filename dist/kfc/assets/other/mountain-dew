<script>
    $(function () {
        var current_pos;

        $("#carousel").owlCarousel({
            items: 1,
            mouseDrag: false,
            touchDrag: false,
            autoplay: false,
            autoplayTimeout: 2000,
            nav: true,
            navText: ["", ""],
            dots: false,
            onInitialized: function (e) {
                current_pos = e.item.index;
            },
            onTranslate: function (e) {
                var direction = e.item.index > current_pos ? 1 : 0,
                    indicator_c = direction ? "next" : "prev",
                    $dots = $(e.currentTarget).find(".owl-dots"),
                    $current_dot = $dots.children().eq(current_pos);

                $current_dot.html('<div class="dot-indicator ' + indicator_c + '">');

                current_pos = e.item.index;

                setTimeout(function () {
                    $current_dot.html("");
                }, 200);
            }
        });
    });

    $(function () {
        var current_pos;

        $("#recommobcarousel").owlCarousel({
            items: 1,
            mouseDrag: false,
            touchDrag: false,
            autoplay: false,
            autoplayTimeout: 2000,
            nav: true,
            navText: ["", ""],
            dots: false,
            onInitialized: function (e) {
                current_pos = e.item.index;
            },
            onTranslate: function (e) {
                var direction = e.item.index > current_pos ? 1 : 0,
                    indicator_c = direction ? "next" : "prev",
                    $dots = $(e.currentTarget).find(".owl-dots"),
                    $current_dot = $dots.children().eq(current_pos);

                $current_dot.html('<div class="dot-indicator ' + indicator_c + '">');

                current_pos = e.item.index;

                setTimeout(function () {
                    $current_dot.html("");
                }, 200);
            }
        });
    });

    $(function () {
        var current_pos;

        $("#carouselrecom").owlCarousel({
            mouseDrag: false,
            touchDrag: false,
            autoplay: false,
            autoplayTimeout: 2000,
            nav: true,
            navText: ["", ""],
            dots: false,
            responsive:{
                0:{
                    items:1
                },
                600:{
                    items:1
                },
                1000:{
                    items:1
                }
            },
            onInitialized: function (e) {
                current_pos = e.item.index;
            },
            onTranslate: function (e) {
                var direction = e.item.index > current_pos ? 1 : 0,
                    indicator_c = direction ? "next" : "prev",
                    $dots = $(e.currentTarget).find(".owl-dots"),
                    $current_dot = $dots.children().eq(current_pos);

                $current_dot.html('<div class="dot-indicator ' + indicator_c + '">');

                current_pos = e.item.index;

                setTimeout(function () {
                    $current_dot.html("");
                }, 200);
            }
        });
    });

</script>
