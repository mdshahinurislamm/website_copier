<script>
    function openCloseHam() {
        $(".ham-menu").toggleClass("open");
    };

    $(document).ready(function () {
        $(".menu-row").find("a").click(function () {
            if ($(this).attr("href") == "#") {
                if ($(this).parent().hasClass("active")) {
                    $(this).siblings('ul').slideUp();
                    $(this).closest("ul").find("li").removeClass("active");
                } else {
                    $(this).closest("ul").find("ul").slideUp();
                    $(this).siblings('ul').slideDown();
                    $(this).closest("ul").find("li").removeClass("active");
                    $(this).parent().addClass("active");
                }
            }
        });

        //For Stickey header

        if ($("#secondary-header").length !== 0) {
            window.onscroll = function () {
                myFunction();
            };
            var navbar = document.getElementById("secondary-header");
            var sticky = navbar.offsetTop;

            function myFunction() {
                if (window.pageYOffset >= sticky) {
                    navbar.classList.add("sticky");
                } else {
                    navbar.classList.remove("sticky");
                }
            }
        }

        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c == 'startMyOrderHint') {
                $('#firstTimeUser').removeClass('firstTimeUser');
                $('#firstTimeUser').addClass('secondTimeUser');
            }
        }

    });


    function ShowSavedAddress(flag) {
        if (flag) {
            $('#openSavedAddressModal').addClass('open');
        } else {
            $('#openSavedAddressModal').removeClass('open');
        }

    }


    function hbMenuHide(label, category) {

        if (label) {
            try {
                dataLayer.push({
                    'event': 'menu_navigation',
                    'eventCategory': category,
                    'eventAction': 'Click',
                    'eventLabel': label
                });
            }
            catch (err) {
            }
        }
        $('.onlyMobile_menu').removeClass("right-0");
        $(".mobileMenu_overLay").addClass("d-none");
        $("body").removeClass("hamMenuOpen");
    }
</script>


<script>
    function hbMenuClick() {
        $('.onlyMobile_menu').addClass("right-0");
        $(".mobileMenu_overLay").removeClass("d-none");
        $("body").addClass("hamMenuOpen");
    }

    function hbMenuRemove() {
        $('.onlyMobile_menu').removeClass("right-0");
        $(".mobileMenu_overLay").addClass("d-none");
        $("body").addClass("hamMenuOpen");
    }
</script>




<div class="modal fade" id="locationDialog" data-backdrop="true" data-keyboard="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content modalprop">
            <div class="modal-body">
            </div>
        </div>
    </div>
</div>

<script>
    var textOverImages = document.getElementsByClassName("onClickTextOverImage");
    var previousTextOverImage;
    for (var i = 0; i < textOverImages.length; i++) {
        textOverImages[i].onclick = function () {
            var classes = this.classList;
            if (classes.contains("show")) {
                classes.remove("show");
            } else {
                if (previousTextOverImage != null)
                    previousTextOverImage.classList.remove("show");
                previousTextOverImage = this;
                classes.add("show");
            }
        }
    }

    function stopPropagation(event) {
        event.stopPropagation();
    }
</script>


<!-- Ad silder-->
<script>
    $(function () {
        var current_pos;

        $("#carousel").owlCarousel({
            items: 1,
            mouseDrag: false,
            touchDrag: false,
            autoplay: false,
            autoplayTimeout: 2000,
            nav: true,
            navText: ["", ""],
            dots: false,
            onInitialized: function (e) {
                current_pos = e.item.index;
            },
            onTranslate: function (e) {
                var direction = e.item.index > current_pos ? 1 : 0,
                    indicator_c = direction ? "next" : "prev",
                    $dots = $(e.currentTarget).find(".owl-dots"),
                    $current_dot = $dots.children().eq(current_pos);

                $current_dot.html('<div class="dot-indicator ' + indicator_c + '">');

                current_pos = e.item.index;

                setTimeout(function () {
                    $current_dot.html("");
                }, 200);
            }
        });
    });

    $(function () {
        var current_pos;

        $("#recommobcarousel").owlCarousel({
            items: 1,
            mouseDrag: false,
            touchDrag: false,
            autoplay: false,
            autoplayTimeout: 2000,
            nav: true,
            navText: ["", ""],
            dots: false,
            onInitialized: function (e) {
                current_pos = e.item.index;
            },
            onTranslate: function (e) {
                var direction = e.item.index > current_pos ? 1 : 0,
                    indicator_c = direction ? "next" : "prev",
                    $dots = $(e.currentTarget).find(".owl-dots"),
                    $current_dot = $dots.children().eq(current_pos);

                $current_dot.html('<div class="dot-indicator ' + indicator_c + '">');

                current_pos = e.item.index;

                setTimeout(function () {
                    $current_dot.html("");
                }, 200);
            }
        });
    });

    $(function () {
        var current_pos;

        $("#carouselrecom").owlCarousel({
            mouseDrag: false,
            touchDrag: false,
            autoplay: false,
            autoplayTimeout: 2000,
            nav: true,
            navText: ["", ""],
            dots: false,
            responsive:{
                0:{
                    items:1
                },
                600:{
                    items:1
                },
                1000:{
                    items:1
                }
            },
            onInitialized: function (e) {
                current_pos = e.item.index;
            },
            onTranslate: function (e) {
                var direction = e.item.index > current_pos ? 1 : 0,
                    indicator_c = direction ? "next" : "prev",
                    $dots = $(e.currentTarget).find(".owl-dots"),
                    $current_dot = $dots.children().eq(current_pos);

                $current_dot.html('<div class="dot-indicator ' + indicator_c + '">');

                current_pos = e.item.index;

                setTimeout(function () {
                    $current_dot.html("");
                }, 200);
            }
        });
    });

</script>
