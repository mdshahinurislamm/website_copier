        var store_content = '';
        var counter = 1;
        var start_lat = '';
        var start_lng = '';
        var addedLocations = [];
        for (i = 0; i < locations.length; i++) {
            if (counter == 1) {
                start_lat = locations[i][1];
                start_lng = locations[i][2];
            }
            var location1 = new google.maps.LatLng(start_lat, start_lng);
            var location2 = new google.maps.LatLng(locations[i][1], locations[i][2]);
            var distance = calculateDistance(location1, location2);
            var directionUrl = 'https://www.google.com/maps/dir/?api=1&origin=' + start_lat + ',' + start_lng + '&destination=' + locations[i][1] + ',' + locations[i][2];
            addedLocations.push([locations[i][0], locations[i][1], locations[i][2], locations[i][3], locations[i][4], locations[i][5], locations[i][6], distance, directionUrl]);
            counter++;
        }
        //console.log(addedLocations);

        addedLocations.sort(function(a,b) {
            return a[7]-b[7];
        });

        for (i = 0; i < addedLocations.length; i++) {
            store_content+='<div class="search-loc-add card">';
            store_content+='<div class="row">';
            store_content+='<div class="col-12 location-info">';
            store_content+='<p class="border-bottom"><span class="icon-loc-1"></span></i>  KFC </p>';
            store_content+='<ul>';
            store_content+='<li class="icon-loc-2"><p> '+addedLocations[i][0]+' </p></li>';
            store_content+='<li class="icon-loc-3"><p> '+addedLocations[i][4]+' </p></li>';
            store_content+='<li class="icon-loc-5" ><p> '+addedLocations[i][5]+' </p></li>';
            store_content+='<li class="icon-loc-4a" ><p> '+addedLocations[i][6]+' </p></li>';
            store_content+='<li class="icon-loc-15" ><p> '+addedLocations[i][7]+' km</p></li>';
            store_content+='<li class="icon-loc-11" ><p> <a href="'+addedLocations[i][8]+'" target="_blank">Directions</a> </p></li>';
            store_content+='</ul>';
            store_content+='</div>';
            store_content+='<div class="col-12 rounded-bottom">';
            store_content+='<div class="card-body list-menu-info ">';
            store_content+='&nbsp;';
            store_content+='</div>';
            store_content+='</div>';
            store_content+='</div>';
            store_content+='</div>';
        }

        //alert(store_content);

        $('#stores_content').html(store_content);

        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 10,
          center: new google.maps.LatLng(23.793583890280377, 90.40538021342425),
          mapTypeId: google.maps.MapTypeId.ROADMAP
        });

        var infowindow = new google.maps.InfoWindow();

        var marker, i;


        for (i = 0; i < locations.length; i++) {
          marker = new google.maps.Marker({
            position: new google.maps.LatLng(locations[i][1], locations[i][2]),
            icon:'https://kfcbd.com/frontend/Content/images/marker.png',
            map: map
          });

          google.maps.event.addListener(marker, 'click', (function(marker, i) {
            return function() {
              infowindow.setContent(locations[i][0]+'<br>'+locations[i][4]);
              infowindow.open(map, marker);
            }
          })(marker, i));
        }
        var addressEl = document.getElementById( 'searchInput' );
        var autocomplete = new google.maps.places.Autocomplete(addressEl);
        autocomplete.bindTo('bounds', map);

        //Search the result only in Bangladesh
        autocomplete.setComponentRestrictions(
          {'country': ['bd']});

        // Set the data fields to return when the user selects a place.
        autocomplete.setFields(
          ['address_components', 'geometry', 'icon', 'name']);


        autocomplete.addListener('place_changed', function() {
            var lat, long, address, resultArray, citi;
            //alert();
            //console.log( 'i am searched' );
            var place = autocomplete.getPlace();
            //console.log(place);
            $('#searchLat').val(place.geometry.location.lat());
            $('#searchLng').val(place.geometry.location.lng());
            // If the place has a geometry, then present it on a map.
        });

        $('#locateDetect').on('click',function() {
            //alert();
            var sortedLocations = [];
            for (i = 0; i < locations.length; i++) {
                var beglat = $('#searchLat').val();
                var beglng = $('#searchLng').val();
                var location1 = new google.maps.LatLng(beglat, beglng);
                var location2 = new google.maps.LatLng(locations[i][1], locations[i][2]);
                var distance = calculateDistance(location1, location2);
                var directionUrl = 'https://www.google.com/maps/dir/?api=1&origin=' + start_lat + ',' + start_lng + '&destination=' + locations[i][1] + ',' + locations[i][2];
                sortedLocations.push([locations[i][0], locations[i][1], locations[i][2], locations[i][3], locations[i][4], locations[i][5], locations[i][6], distance, directionUrl]);
            }

            sortedLocations.sort(function(a,b) {
                return a[7]-b[7];
            });

            var store_content = '';

            for (i = 0; i < sortedLocations.length; i++) {
                store_content+='<div class="search-loc-add card">';
                store_content+='<div class="row">';
                store_content+='<div class="col-12 location-info">';
                store_content+='<p class="border-bottom"><span class="icon-loc-1"></span></i>  KFC </p>';
                store_content+='<ul>';
                store_content+='<li class="icon-loc-2"><p> '+sortedLocations[i][0]+' </p></li>';
                store_content+='<li class="icon-loc-3"><p> '+sortedLocations[i][4]+' </p></li>';
                store_content+='<li class="icon-loc-5" ><p> '+sortedLocations[i][5]+' </p></li>';
                store_content+='<li class="icon-loc-4a" ><p> '+sortedLocations[i][6]+' </p></li>';
                store_content+='<li class="icon-loc-15" ><p> '+sortedLocations[i][7]+' km</p></li>';
                store_content+='<li class="icon-loc-11" ><p> <a href="'+sortedLocations[i][8]+'" target="_blank">Directions</a> </p></li>';
                store_content+='</ul>';
                store_content+='</div>';
                store_content+='<div class="col-12 rounded-bottom">';
                store_content+='<div class="card-body list-menu-info ">';
                store_content+='&nbsp;';
                store_content+='</div>';
                store_content+='</div>';
                store_content+='</div>';
                store_content+='</div>';
            }

            //alert(store_content);

            $('#stores_content').html(store_content);
        });

        //calculates distance between two points in km's
        function calculateDistance(pointer_1, pointer_2) {
            var dist = (google.maps.geometry.spherical.computeDistanceBetween(pointer_1, pointer_2) / 1000).toFixed(2);
            return parseFloat(dist);

        }

  </script>

